{
    "name": "levitated/notifications",
    "description": "Email/SMS notifications",
    "authors": [
        {
            "name": "Levitated",
            "email": "jan@levitated.pl"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*",
        "aws/aws-sdk-php-laravel": "1.*",
        "barryvdh/laravel-twigbridge": "*",
        "erusev/parsedown": "*",
        "levitated/helpers": "1.0.*",
        "twilio/sdk": "3.12.*"
    },
    "require-dev": {
    "mockery/mockery": "dev-master@dev",
        "hamcrest/hamcrest-php": "*"
    },
    "autoload": {
        "psr-0": {
            "Levitated\\Notifications": "src/"
        },
        "classmap": [
            "tests/TestCase.php",
            "src/models"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
